    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist build",
    "test": "echo \"Error: no tests specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "rollup --c rollup.config.js && tsc -d --emitDeclarationOnly --declarationDir dist/types",
    "zip-source": "shx rm -rf ./build/source && shx mkdir -p ./build/source && copyfiles -a ./* ./.storybook ./build/source && shx cp -r ./src ./build/source && shx cp -r ./.storybook ./build/source/.storybook && shx mkdir -p ./dist/source && cd build && bestzip --force node ../dist/source/source.zip * .* && cd ..",
    "dist": "npm run clean && npm run build && npm run zip-source"
  },
